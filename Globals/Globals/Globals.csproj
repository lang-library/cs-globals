  </ItemGroup>
  <ItemGroup>
    <Compile Include="..\..\Globals.Slim\Globals.Slim\ApplicationIdleTimer.cs" Link="ApplicationIdleTimer.cs" />
    <Compile Include="..\..\Globals.Slim\Globals.Slim\Dirs.cs" Link="Dirs.cs" />
